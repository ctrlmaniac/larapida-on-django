{
    "name": "larapida-on-django",
    "private": true,
    "workspaces": [
        "packages/*",
        "clients/*"
    ],
    "scripts": {
        "collectclients": "python collectclients.py",
        "build:clients": "npx lerna exec --concurrency 1 -- yarn build",
        "runserver": "python webapp/manage.py runserver",
        "makemigrations": "python webapp/manage.py makemigrations",
        "migrate": "python webapp/manage.py migrate",
        "check:deploy": "python webapp/manage.py check --deploy",
        "start:www": "yarn workspace www start",
        "export:requirements": "poetry export -f requirements.txt --output requirements.txt"
    },
    "dependencies": {
        "npm": "^8.12.1"
    },
    "devDependencies": {
        "lerna": "^5.1.1"
    }
}
